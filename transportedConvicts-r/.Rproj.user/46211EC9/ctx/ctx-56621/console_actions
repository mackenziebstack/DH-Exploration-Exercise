        "require(topicmodels)",
        "> ",
        "# number of topics",
        "> ",
        "K <- 15",
        "> ",
        "# set random number generator seed",
        "> ",
        "# for purposes of reproducibility",
        "> ",
        "set.seed(9161)",
        "> ",
        "# compute the LDA model, inference via 1000 iterations of Gibbs sampling",
        "> ",
